<table>
    <tr>
        <th>Name</th>
        <th>Year</th>
        <th>Img</th>
        <th>Count</th>
        <th>Price</th>
    </tr>
    {{#each card.card}}
    <tr>
        <td>{{name}}</td>
        <td>{{year}}</td>
        <td>{{img}}</td>
        <td>{{count}}</td>
        <td class="price">{{price}}</td>
    </tr>
    {{/each}}
    <tr>
        <td></td>
        <td></td>
        <td></td>
        <td>Total count: {{card.count}}</td>
        <td>Total price: <span class="price">{{card.price}}</span></td>
    </tr>
</table>